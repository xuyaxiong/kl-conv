  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: utils",
      "type": "debugpy",
      "request": "launch",
      "module": "src.lib.utils",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: gui",
      "type": "debugpy",
      "request": "launch",
      "module": "src.mode.gui",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: cli",
      "type": "debugpy",
      "request": "launch",
      "module": "src.mode.cli",
      "console": "integratedTerminal",
      "args": [
        "--cli",
        "-f",
        "./assets/PS_DLL.h"
      ]
    },
    {
      "name": "Python: app",
      "type": "debugpy",
      "request": "launch",
      "module": "src.app",
      "console": "integratedTerminal"
    },
  ]
}