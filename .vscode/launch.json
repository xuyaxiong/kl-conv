  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: utils",
      "type": "debugpy",
      "request": "launch",
      "module": "src.lib.utils",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: app-gui",
      "type": "debugpy",
      "request": "launch",
      "module": "src.app",
      "console": "integratedTerminal"
